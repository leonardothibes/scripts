#!/bin/bash

clear

redis-cli flushall
curl --request DELETE http://127.0.0.1:9200/catalog/products

WORKER=$HOME/workspace/shop-worker
go run $WORKER/src/main.go --config-file=$WORKER/config/parameters.yml
#go run $WORKER/src/main.go --config-file=$WORKER/config/parameters.yml --importers=brands,categories,products